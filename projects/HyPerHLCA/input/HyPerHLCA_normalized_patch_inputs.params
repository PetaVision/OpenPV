KernelConn "V1ToRecon" = {
    preLayerName = "V1";
    postLayerName = "Recon";
    channelCode = 0;
    nxp = 12;
    nyp = 12;
    nfp = 1;
    numAxonalArbors = 1;
    initFromLastFlag = 0;
    
    //weightInitType = "FileWeight";
    //initWeightsFile = "/Users/garkenyon/workspace/HyPerHLCA2/Checkpoints/Checkpoint220000/V1ToRecon_W.pvp";
    weightInitType = "UniformRandomWeight";
    wMinInit = -1.0;
    wMaxInit = 1.0;
    sparseFraction = .90;
    
    strength = 1.0;  
    normalize = 1.0;
    normalize_zero_offset = true;
    normalize_max = false;
    normalize_cutoff = 0.0;
    normalize_RMS_amp = false;
    symmetrizeWeights = false;
    normalizeTotalToPost = false;

    preActivityIsNotRate = false;  // should help make response more indepenent of time step size dt
