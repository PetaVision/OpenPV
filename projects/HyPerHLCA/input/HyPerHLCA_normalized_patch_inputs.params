ANNLayer "Ganglion" = {
    restart = 0;
    nxScale = 1;
    nyScale = 1;
    nf = 1;
    marginWidth = 6;
    writeStep = 100.0;
    initialWriteTime = 99.0;
    mirrorBCflag = 1;
    writeSparseActivity = 0;

    InitVType = "ZeroV";

    VThresh = -infinity;
    VMax = infinity;
    VMin = -infinity;
    VShift = 0;
    
    phase = 2;
};


ANNSquaredLayer "GanglionSquared" = {
    restart = 0;
    nxScale = 1;
    nyScale = 1;
    nf = 1;
    marginWidth = 6;
    writeStep = 100.0;
    initialWriteTime = 99.0;
    mirrorBCflag = 1;
    writeSparseActivity = 0;

    InitVType = "ZeroV";

    VThresh = -infinity;
    VMax =  infinity;
    VMin = -infinity;
    VShift = 0;
    
    phase = 3;
};


ANNLayer "Recon" = {
    restart = 0;
    nxScale = 1;
    nyScale = 1;
    nf = 1;
    marginWidth = 6;
    writeStep = 100.0;
    initialWriteTime = 99.0;
    mirrorBCflag = 1;
    writeSparseActivity = 0;

    InitVType = "ZeroV";

    VThresh = -infinity; //-infinity;
    VMax = infinity; //infinity;  // prevent reconstruction from exceeding reasonable bounds
    VMin = -infinity; //-infinity;
    VShift = 0;
    
    phase = 2; // could be 5..., but that should produce the same delays
};


HyPerLCALayer "V1" = {
    restart = 0;
    nxScale = 0.25;
    nyScale = 0.25;
    nf = 512;
    marginWidth = 1;
    writeStep = -1.0;
    initialWriteTime = 99.0;
    mirrorBCflag = 1;
    writeSparseActivity = 0;

    InitVType = "UniformRandomV";
    minV = 0.0;
    maxV = 1.05;
    
    timeConstantTau = 100.0;
    
    VThresh = 1.0;
    VMax = infinity; //infinity;
    VMin = 0;
    VShift = 0.0;  // 0 -> hard threshold, 1 -> soft threshold

    phase = 4;
};




