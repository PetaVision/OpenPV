// connections

IdentConn "MovieToError" = {
    preLayerName = "Image";
    postLayerName = "movieError";
    channelCode = 0;
    delay = 0;
    writeStep = -1;
};

KernelConn "V1ToMovieError" = {
    preLayerName = "V1";
    postLayerName = "movieError";
    channelCode = 1;
    nxp = 32;
    nyp = 32;
    numAxonalArbors = 1;
    initFromLastFlag = 0;
    
    
    weightInitType = "FileWeight";
    initWeightsFile = "/Users/bcrocker/Documents/workspace/HyPerHLCA2/input/w_imprintedweights.pvp";

    //weightInitType = "UniformRandomWeight";
    //wMinInit = -1.0;
    //wMaxInit = 1.0;
    //sparseFraction = 0.9;
   
    strength = 0.5;  
    normalizeMethod = "normalizeL2";
    minL2NormTolerated = 0;
    normalizeArborsIndividually = 0;
    normalize_cutoff = 0.0;
    normalizeFromPostPerspective = false;
    symmetrizeWeights = false;

    preActivityIsNotRate = false;  // should help make response more indepenent of time step size dt
    keepKernelsSynchronized = true; 
    combine_dW_with_W_flag = false; 

    writeStep = 1000000;
    writeCompressedWeights = false;
    writeCompressedCheckpoints = false;
    plasticityFlag = true;
    weightUpdatePeriod = 1000.0;
    initialWeightUpdateTime = 999.0;
    initialWriteTime = 0.0;
    dWMax = 0.0005;
    selfFlag = false;
    shmget_flag = false;

    delay = 0;

    shrinkPatches = false;
};

TransposeConn "MovieErrorToV1" = {
	preLayerName = "movieError";
	postLayerName = "V1";
	channelCode = 0;

	originalConnName = "V1ToMovieError";
	     
	selfFlag = false;
	preActivityIsNotRate = false;	
	writeStep = -1;
	writeCompressedCheckpoints = false;
	shmget_flag = false;
	
	delay = 0;
};

CloneKernelConn "V1ToMovieRecon" = {
   preLayerName = "V1";
   postLayerName = "movieRecon";
   channelCode = 0;
   writeStep = -1;
   originalConnName = "V1ToMovieError";
   selfFlag = false;
   delay = 0;
