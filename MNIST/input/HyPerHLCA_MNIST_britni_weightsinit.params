    VThresh = -infinity;
    VMax =  infinity;
    VMin = -infinity;
    VShift = 0;
    
    phase = 3;
};



HyPerLCALayer "V1" = {
    restart = 0;
    nxScale = 0.03125;
    nyScale = 0.03125;
    nf = 1400;
    marginWidth = 0;
    initialWriteTime = 0;
    writeStep = 1000000;
    mirrorBCflag = 1;
    writeSparseActivity = 1;

    InitVType = "UniformRandomV";
    minV = -1.0;
    maxV = 0.35;
    
    timeConstantTau = 100.0;
    
    VThresh = 0.3;
    VMax = infinity;
    VMin = 0;
    VShift = 0.0;

    phase = 4;
};

