    inverseFlag = false;
    normalizeLuminanceFlag = true;
    writeImages = false;
    autoResizeFlag = true;
    offsetX = 0;
    offsetY = 0;
    randomMovie = 0;
    phase = 0;
};

LabelLayer "labels" = {
    nf = 10;
    labelStart = 31;
    labelLength = 1;
    movieLayerName = "Image";
    phase = 1;
    marginWidth = 0;
    writeStep = 1000000;    
};

ANNLayer "movieError" = {
    restart = 0;
    nxScale = 1;
    nyScale = 1;
    nf = 1;
    marginWidth = 6;
    writeStep = -1;
    mirrorBCflag = 0;
    writeSparseActivity = 0;

    InitVType = "ZeroV";
    VThresh = -infinity;
    VMax =  infinity;
    VMin = -infinity;
    VShift = 0;
    
