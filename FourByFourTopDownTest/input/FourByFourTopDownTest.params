    preLayerName = "AnaRetina";
    postLayerName = "Reconstructed Image";

    channelCode = 1;
    
    nxp = 1;
    nyp = 1;
    nfp = 1;

    writeStep = -1;
    writeCompressedWeights = true;
    
    delay = 0;
};

