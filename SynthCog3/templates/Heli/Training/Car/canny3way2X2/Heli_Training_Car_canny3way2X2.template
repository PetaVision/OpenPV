// Clique connections

CliqueConn "L1ToL1Post" = {
   preLayerName = "L1";
   postLayerName = "L1Post";
   channelCode = 2;  // not received if plasticityFlag == true
   nxp = 33;
   nyp = 33;
   nfp = 8;
   cliqueSize = 2;
   numAxonalArbors = 8712;
   initFromLastFlag = 0;  // 1;  // restart
   writeStep = -1;

   weightInitType = "FileWeight";  
   combineWeightFiles = true; //false; // 
   $$$_numWeightFilesTarget_$$$
   $$$_initWeightsFileL1ToL1PostTarget_$$$ 
   useListOfArborFiles = false;

   normalize = 0.0;
   
   plasticityFlag = 1;
   keepKernelsSynchronized = false; // true; 
   combine_dW_with_W_flag = true; //false; // 
   shrinkPatches = false;
   writeCompressedWeights = 0.0;
   writeCompressedCheckpoints = 0.0;
   stochasticReleaseFlag = false;
   weightUpdatePeriod = 1.0;

   delay = 0;    
    
   preActivityIsNotRate = false;
   selfFlag = true;
   shmget_flag = false;
   initialWeightUpdateTime = 0.0;
};


