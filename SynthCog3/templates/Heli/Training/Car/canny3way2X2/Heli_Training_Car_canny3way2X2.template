   weightInit = 1;
   strength = 1.0;       
   normalize = 1.0;
   normalize_zero_offset = 0.0;
   normalize_max = 0.0;
   normalize_cutoff = 0;
   normalizeTotalToPost = true;
   preActivityIsNotRate = false;

   shrinkPatches = false;
   writeCompressedWeights = 0.0;
   writeCompressedCheckpoints = 0.0;
   selfFlag = false;
   shmget_flag = false;
   plasticityFlag = 0;
   stochasticReleaseFlag = false;
    
   delay = 0;    
