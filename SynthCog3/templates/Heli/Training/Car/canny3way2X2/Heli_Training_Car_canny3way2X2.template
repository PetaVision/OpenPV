// Mask connections

// copy edge detections
IdentConn "L1ToL1PostVertical" = {
    preLayerName = "L1";
    postLayerName = "L1Post";
    channelCode = 0;
    writeStep = -1;
    initFromLastFlag = 0;
    weightInitType = "IdentWeight";

    delay = 0;
};


// mask non-amoeba edges
KernelConn "MasktoL1Post" = {
   preLayerName = "Mask";
   postLayerName = "L1Post";
   channelCode = 1;
   nxp = 1; 
   nyp = 1; 
   nfp = 8;
   numAxonalArbors = 1;
   initFromLastFlag = 0;  // 1;  // restart
   writeStep = -1;
   
   weightInitType = "UniformWeight";
   weightInit = 1;
   strength = 1.0;              
   normalize = 1.0;
