//
// clique_template.params
//  - template input parameters for learning Cat 3way clique kernels 
//

HyPerCol "column" = {
   nx = 360;   // until we get going with images
   ny = 256;
   dt = 1.0;
   randomSeed = 111111111;  // if not set here,  clock time is used to generate seed
   numSteps = 100;
   $$$_outputPath_$$$
   progressStep = 20;
   filenamesContainLayerNames = false;
   checkpointRead = false;
   checkpointWrite = false;
   $$$_printParamsFilename_$$$
   writeProgressToErr = true;
   filenamesContainConnectionNames = false;
};

