HyPerLCALayer "V1_S2" = {
    restart             = 0;
    nxScale             = 0.5;
    nyScale             = 0.5;
    nf                  = 16;
    numChannels         = 1; // Only excitatory channel
    numWindowX          = 1;
    numWindowY          = 1;
    writeStep           = 400;
    initialWriteTime    = 400;
    mirrorBCflag        = 0;
    valueBC             = 0;
    writeSparseActivity = true;
    writeSparseValues   = true;
